{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "no-param-reassign": 0,
    "no-console": 0,
    "strict": [0, "global"],
    "react/prefer-stateless-function": 0,
    "no-multi-spaces": ["error", { "exceptions": { "VariableDeclarator": true } }]
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "$": true
  }
}
