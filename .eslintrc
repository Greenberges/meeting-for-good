{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "no-param-reassign": 0,
    "no-console": 0,
    "strict": [0, "global"],
    "react/prefer-stateless-function": 0,
    "no-multi-spaces": ["error", { "exceptions": { "VariableDeclarator": true } }],
    "no-shadow": 0,
    "no-underscore-dangle": 0
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "$": true,
    "Materialize": true,
    "wNumb": true
  },
  "parser": "babel-eslint"
}
